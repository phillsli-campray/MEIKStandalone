    <!--这是正常的启动窗体方式，没有过场画面-->
<Application x:Class="MEIKReport.App"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"  StartupUri="MainWindow.xaml" ShutdownMode="OnMainWindowClose">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--The source of the resource dictionary is spcified-->
                <!--We can add many number of references over here as the source-->
                <ResourceDictionary Source="Styles/ReportStyle.xaml"/>
                <ResourceDictionary Source="Resources/StringResource.xaml" />
                <!--<ResourceDictionary Source="Resources/StringResource.zh-CN.xaml" />-->

                <!--<ResourceDictionary Source="Styles/ReportData.xaml"/>-->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
